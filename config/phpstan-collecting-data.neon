parameters:
    # avoid any other rules
    customRulesetUsed: true

    bootstrapFiles:
        - bin/autoload.php

    excludePaths:
        - "*/Fixture/*"
        - "*/Fixtures/*"

rules:
    - Rector\ArgTyper\PHPStan\Rule\CollectFuncCallArgTypesRule
    - Rector\ArgTyper\PHPStan\Rule\CollectCallLikeArgTypesRule

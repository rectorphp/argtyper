parameters:
    # avoid need of level
    customRulesetUsed: true

services:
    -
        class: TomasVotruba\SherlockTypes\PHPStan\Rule\DumpPHPUnitAssertTypesRule
        tags:
            - phpstan.rules.rule

    -
        class: TomasVotruba\SherlockTypes\PHPStan\Collectors\PHPUnitAssertMethodCallCollector
        tags:
            - phpstan.collector

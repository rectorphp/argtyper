parameters:
    # avoid need of level
    customRulesetUsed: true

services:
    -
        class: Rector\ArgTyper\PHPStan\Collectors\MethodCallArgTypeCollector
        tags:
            - phpstan.collector

    -
        class: Rector\ArgTyper\PHPStan\Collectors\StaticCallArgTypeCollector
        tags:
            - phpstan.collector

rules:
    - Rector\ArgTyper\PHPStan\Rule\DumpMethodCallArgTypesRule